            "content": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n  <head>\r\n    <meta charset=\"UTF-8\" />\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\" />\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\r\n    <link\r\n      rel=\"stylesheet\"\r\n      href=\"https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css\"\r\n      integrity=\"sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm\"\r\n      crossorigin=\"anonymous\"\r\n    />\r\n    <script\r\n      src=\"https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js\"\r\n      integrity=\"sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl\"\r\n      crossorigin=\"anonymous\"\r\n    ></script>\r\n    <link\r\n      rel=\"stylesheet\"\r\n      href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\"\r\n    />\r\n    <link\r\n      rel=\"stylesheet\"\r\n      href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css\"\r\n    />\r\n    <link rel=\"stylesheet\" href=\"./css/profile.css\" />\r\n    <title>Document</title>\r\n  </head>\r\n  <body style=\"background-color: rgb(231, 230, 230);\">\r\n    <div class=\"info-contact\">\r\n      <div class=\"content\">\r\n          \r\n          <h3>My profile</h3>\r\n      </div>\r\n      </div>\r\n    <div class=\"pcontainer\">\r\n      <div class=\"row\">\r\n        <div class=\"col-12 col-md-5 col-lg-4 mb-3 justify-center\">\r\n          <div class=\"profile-links pb-0 pt-3\">\r\n            <div class=\"upload\">\r\n              <div\r\n                id=\"avatar\"\r\n                style=\"border: 1px solid; width: 70px; height: 70px; border-radius: 50%; display: inline-block; margin-bottom: 5px; box-shadow: 1px -2px 9px 1px rgb(19 19 19 / 53%), 0 3px 10px 0 rgb(19 19 19 / 53%);\"\r\n              >\r\n                <strong\r\n                  style=\"display: flex; justify-content: center; margin-top: 25px;\"\r\n                  id=\"userName\"\r\n                >                       \r\n                </strong>\r\n              </div>\r\n\r\n              <img\r\n                id=\"output\"\r\n                :src=\"profileImageSrc\"\r\n                width=\"100\"\r\n                height=\"100\"\r\n                alt=\"\"\r\n              />\r\n              <div class=\"round\">\r\n                <input\r\n                  type=\"file\"\r\n                  accept=\"image/*\"\r\n                  onchange=\"loadFile(event)\"\r\n                />\r\n                <i class=\"fa fa-plus-circle\" style=\"color: #000000;\"></i>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"content-profile\">\r\n              <h3 class=\"mt-2\" id=\"userFullName\"></h3>\r\n            </div>\r\n\r\n            <hr />\r\n            <li\r\n              id=\"MyProfile\"\r\n              class=\"mx-2 profile-btns\"\r\n            >\r\n              <i class=\"mx-2 fa fa-person mr-1\"></i\r\n              ><a>My profile</a>\r\n            </li>\r\n\r\n            <li\r\n              class=\"mx-2 profile-btns\"\r\n              id=\"dashboard\"\r\n            >\r\n              <i class=\"fa fa-dashboard mx-2\"></i\r\n              ><a>Dashbord</a>\r\n            </li>\r\n\r\n            <li\r\n              class=\"mx-2 profile-btns\"\r\n              id=\"SavePattents\"\r\n            >\r\n              <i class=\"fa fa-save mx-2\"></i\r\n              ><a\r\n                >Save pattents\r\n                <div class=\"badge\">13</div>\r\n              </a>\r\n            </li>\r\n\r\n            <li\r\n              class=\"mx-2 profile-btns\"\r\n              id=\"SavedSearchResults\"\r\n            >\r\n              <i class=\"fa fa-bookmark mx-2\"></i\r\n              ><a\r\n                >Saved search results\r\n\r\n                <div class=\"badge\">13</div>\r\n              </a>\r\n            </li>\r\n\r\n            <li class=\"mx-2 profile-btns\">\r\n              <i id=\"message\" class=\"fa fa-message mx-2\"></i\r\n              ><a\r\n                >Messages\r\n                <div class=\"badge\">13</div>\r\n              </a>\r\n            </li>\r\n\r\n            <li class=\"mx-2 profile-btns\">\r\n              <i id=\"properties\" class=\"fa fa-user-edit mx-2\"></i\r\n              ><a>Properties </a>\r\n            </li>\r\n\r\n            <li class=\"mx-2 profile-btns\">\r\n              <i id=\"ChangePassword\" class=\"fa fa-exchange-alt mx-2\"></i\r\n              ><a>Change password</a>\r\n            </li>\r\n\r\n            <button class=\"mt-3\">\r\n              <i style=\"color: white;\" class=\"fa fa-power-off mx-auto\"></i>\r\n            </button>\r\n          </div>\r\n        </div>\r\n\r\n                <!--right content in profile page -->\r\n                <div class=\"col-12 col-md-7\">\r\n                  <div class=\"form-main-right\">\r\n                    <div class=\"title-profile-box\">\r\n                      <h3 class=\"my-3\">My Accounts</h3>\r\n                    </div>\r\n        \r\n                    <form action=\"\">\r\n                      <div class=\"row\">\r\n                        <div class=\"col-12 col-lg-6\">\r\n                          <div class=\"form-element\">\r\n                            <label for=\"name\" class=\"mx-3\" style=\"display: inline;\"\r\n                              >Name</label\r\n                            >\r\n        \r\n                            <input\r\n                              type=\"text\"\r\n                              class=\"input-field mt-2\"\r\n                              placeholder=\"Your Name\"\r\n                              required\r\n                            />\r\n                          </div>\r\n                        </div>\r\n        \r\n                        <div class=\"col-12 col-lg-6\">\r\n                          <div class=\"form-element\">\r\n                            <label for=\"email\" class=\"mx-3\" style=\"display: inline;\"\r\n                              >Email</label\r\n                            >\r\n        \r\n                            <input\r\n                              type=\"email\"\r\n                              class=\"input-field mt-2\"\r\n                              placeholder=\"Your Email\"\r\n                              required\r\n                            />\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n        \r\n                      <div class=\"row\">\r\n                        <div class=\"col-12 col-lg-6\">\r\n                          <div class=\"form-element\">\r\n                            <label for=\"title\" class=\"mx-3\" style=\"display: inline;\"\r\n                              >Your Title</label\r\n                            >\r\n\r\n                            <select class=\"input-field mt-2\" placeholder=\"Your Title\" required>\r\n                              <option></option>\r\n                            </select>\r\n                          </div>\r\n                        </div>\r\n        \r\n                        <div class=\"col-12 col-lg-6\">\r\n                          <div class=\"form-element\">\r\n                            <label for=\"phone\" class=\"mx-3\" style=\"display: inline;\"\r\n                              >phone</label\r\n                            >\r\n        \r\n                            <input\r\n                              type=\"text\"\r\n                              class=\"input-field mt-2\"\r\n                              placeholder=\"Your phone\"\r\n                              required\r\n                            />\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n        \r\n                      <div class=\"row\">\r\n                        <div class=\"col-12 col-lg-6\">\r\n                          <div class=\"form-element\">\r\n                            <label\r\n                              for=\"enterprise\"\r\n                              class=\"mx-3\"\r\n                              style=\"display: inline;\"\r\n                              >enterprise</label\r\n                            >\r\n        \r\n                            <select class=\"input-field mt-2\" required>\r\n                              <option></option>\r\n                            </select>\r\n                          </div>\r\n                        </div>\r\n        \r\n                        <div class=\"col-12 col-lg-6\">\r\n                          <div class=\"form-element\">\r\n                            <label\r\n                              for=\"level of education\"\r\n                              class=\"mx-3\"\r\n                              style=\"display: inline;\"\r\n                              >level of education</label\r\n                            >\r\n        \r\n                            <select class=\"input-field mt-2\" required>\r\n                              <option></option>\r\n                            </select>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n        \r\n                      <div class=\"row\">\r\n                        <div class=\"col-12 col-lg-12\">\r\n                          <div class=\"form-element\">\r\n                            <label for=\"favorites\" class=\"mx-3\" style=\"display: inline;\"\r\n                              >favorites</label\r\n                            >\r\n        \r\n                            <select class=\"input-field mt-2\" required>\r\n                              <option></option>\r\n                            </select>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n        \r\n                      <div class=\"row\">\r\n                        <div class=\"col-lg-12\">\r\n                          <div class=\"form-element\">\r\n                            <label for=\"About\" class=\"mx-3\" style=\"display: inline;\"\r\n                              >About</label\r\n                            >\r\n        \r\n                            <textarea\r\n                              rows=\"3\"\r\n                              type=\"text\"\r\n                              class=\"input-field mt-2\"\r\n                              placeholder=\"About\"\r\n                              required\r\n                            ></textarea>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n        \r\n                      <div class=\"title-profile-box\">\r\n                        <h3 class=\"my-3\">Social accounts</h3>\r\n                      </div>\r\n        \r\n                      <div class=\"details\">\r\n                        <div class=\"row\">\r\n                          <div class=\"input-group\">\r\n                            <label for=\"subject\"> Facebook</label>\r\n        \r\n                            <input type=\"text\" id=\"subject\" required />\r\n                          </div>\r\n        \r\n                          <div class=\"input-group\">\r\n                            <label for=\"subject\"> Twitter </label>\r\n        \r\n                            <input type=\"text\" id=\"subject\" required />\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n        \r\n                      <div class=\"details\">\r\n                        <div class=\"row\">\r\n                          <div class=\"input-group\">\r\n                            <label for=\"subject\"> Google plus</label>\r\n        \r\n                            <input type=\"text\" id=\"subject\" required />\r\n                          </div>\r\n        \r\n                          <div class=\"input-group\">\r\n                            <label for=\"subject\"> linkedin </label>\r\n        \r\n                            <input type=\"text\" id=\"subject\" required />\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n        \r\n                      <button class=\"my-3\">save changes</button>\r\n                    </form>\r\n                  </div>\r\n                </div>\r\n    </div>\r\n    <!--contact box-->\r\n    <div class=\"row responsive-contact-us-part\">\r\n      <div class=\"col-12\">\r\n          <div class=\"contact-box-base\">\r\n            <div class=\"contact-title-base\">\r\n              <h3>Do you have any questions?</h3>\r\n    \r\n              <h4>We will help you?</h4>\r\n            </div>\r\n    \r\n            <button class=\"btn-contact\">\r\n              Contact Us\r\n            </div>\r\n          </div>\r\n      </div>\r\n    </div>\r\n <!--end contact box-->\r\n\r\n <script src=\"https://code.jquery.com/jquery-3.3.1.slim.min.js\" integrity=\"sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo\" crossorigin=\"anonymous\"></script>\r\n <script src=\"https://cdn.jsdelivr.net/npm/popper.js@1.14.7/dist/umd/popper.min.js\" integrity=\"sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1\" crossorigin=\"anonymous\"></script>\r\n <script src=\"https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/js/bootstrap.min.js\" integrity=\"sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM\" crossorigin=\"anonymous\"></script>\r\n <script src=\"./js/jquery-3.6.0.min.js\"></script>\r\n\r\n <script>\r\n     let image = document.getElementById('output');\r\n\r\n     document.getElementById('userFullName').innerHTML = 'Tom Paterson';\r\n\r\n     document.getElementById('userName').innerHTML = `${'Tom'.charAt(0)} ${'Paterson'.charAt(0)}`;\r\n\r\n     window.addEventListener(\"load\", (event) => {\r\n         if(!image.src) document.getElementById('output').style.display = 'none'\r\n     });\r\n\r\n     var loadFile = function(event) {\r\n         image.src=URL.createObjectURL(event.target.files[0]);\r\n         if(image.src) {\r\n             document.getElementById('avatar').style.display = 'none';\r\n             document.getElementById('output').style.display = 'inline'\r\n         }\r\n     };\r\n\r\n     const callToActionBtns = document.querySelectorAll(\".profile-btns\");\r\n     callToActionBtns.forEach((btn) => {\r\n         btn.addEventListener(\"click\", (e) => {\r\n             e.preventDefault();\r\n             callToActionBtns.forEach(f => f.classList.remove('active-class'));\r\n             e.target.classList.toggle(\"active-class\");\r\n             console.log(btn)\r\n         });     \r\n     });\r\n </script>\r\n  </body>\r\n</html>\r\n"
